{
    "compilerOptions": {
      "outDir": "dist",
      "incremental": true,
      "target": "esnext",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": ["dist", "node_modules"]
  } 